{
  "name": "demo-webpack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "/build": "webpack --profile --progress --config webpack.prod.js",
    "test": "jest",
    "prettier": "prettier --write \"**/*.{tsx,ts}\" ",
    "lint": "eslint \"**/*.{tsx,ts}\" ",
    "analyze": "source-map-explorer 'dist/static/js/*.js'"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-loader": "^9.1.2",
    "browserslist": "^4.21.5",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "source-map-explorer": "^2.5.3",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3",
    "webpack": "^5.77.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "file-saver": "^2.0.5",
    "immer": "^9.0.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "redux": "^4.2.1",
    "styled-components": "^5.3.9",
    "web-vitals": "^3.3.0",
    "zustand": "^4.3.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.ts"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.{ts,tsx}"
    ],
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/__mocks__/css-mock.js",
      "\\.(jpg|png|svg)$": "<rootDir>/__mocks__/file-mock.js",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts?$": ["ts-jest", { "isolatedModules": true }]
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
